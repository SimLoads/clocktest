__lua__
function _init()
    coords={
        x=1,
        y=1,
        dy=9,
        bry=17,
        
    }
end
function _update60()
    cls()
    drawmap()
    clockfunc()
    if btn(⬆️) then
    	poke(0x5e00, (peek(0x5e00)+16))
    end
    if btn(⬇️) then
    	print(peek(0x5e00))
    end
    
end
function drawmap()
	map(0,0,0,0,128,128)
end

function clockfunc()
    print(stat(6),coords.x,coords.bry)
    print(""..stat(91).."/"..stat(92).."/"..stat(90),coords.x,coords.y)
  	print(""..stat(93)..":"..stat(94)..":"..stat(95),coords.x,coords.dy)
end

